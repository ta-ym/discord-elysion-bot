{
  "name": "discord-elysion-bot",
  "version": "1.0.0",
  "description": "A Discord bot built with TypeScript and discord.js",
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc",
    "start": "node dist/index.js",
    "dev": "ts-node src/index.ts",
    "watch": "tsc --watch",
    "deploy-commands": "ts-node src/deploy-commands.ts",
    "migrate": "ts-node src/migrate.ts",
    "backup": "ts-node -e \"import('./src/backup.js').then(m => new m.DatabaseBackup().createBackup())\"",
    "lint": "eslint src/**/*.ts",
    "lint:fix": "eslint src/**/*.ts --fix"
  },
  "keywords": [
    "discord",
    "bot",
    "typescript",
    "discord.js"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "discord.js": "^14.14.1",
    "dotenv": "^16.3.1",
    "sqlite3": "^5.1.6",
    "pg": "^8.11.3"
  },
  "devDependencies": {
    "@types/node": "^20.9.0",
    "@types/sqlite3": "^3.1.11",
    "@types/pg": "^8.10.9",
    "@typescript-eslint/eslint-plugin": "^6.10.0",
    "@typescript-eslint/parser": "^6.10.0",
    "eslint": "^8.53.0",
    "ts-node": "^10.9.1",
    "typescript": "^5.2.2"
  },
  "engines": {
    "node": ">=16.9.0"
  }
}